{{!< default}}

<h1>{{tag.name}}</h1>
    {{! Each post will be output using this markup }}
   {{#foreach posts}}
<section id="{{id}}" class="{{post_class}}">
    <h3 class="post-title"><a href="{{url}}">{{title}}</a> <span class="separator">&middot;</span> {{date published_at format='MMMM Do, YYYY'}}</h3>
</section>
{{/foreach}}

    {{!! After all the posts, we have the previous/next pagination links }}
    {{pagination}}