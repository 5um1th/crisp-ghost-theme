{{!< default}}
<h1 class="post-title">{{tag.name}}</h1> 
{{#foreach posts}}
<section id="{{id}}" class="{{post_class}}">
	<h3 class="post-title"><a href="{{url}}">{{title}}</a> <span class="separator">&middot;</span> {{date published_at format='MMMM Do, YYYY'}}<span class="taglist">{{tags separator=" " prefix=" &middot; "}}</span></h3>
</section>
{{/foreach}}
{{pagination}}